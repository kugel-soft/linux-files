# Start pulseaudio deamon
(sleep 1s && start-pulseaudio-x11 && sleep 2s && volumeicon) &

# composite manager
xcompmgr &

# Panel
(sleep 1 && tint2) &

# Desktop icons
# (sleep 1 && idesk) &

# Notifications
/usr/lib/notification-daemon/notification-daemon &

# Numlock
numlockx &

# file manager
#(sleep 1 && thunar --daemon) &

# Policy Kit
(sleep 1 && /usr/lib/policykit-1-gnome/polkit-gnome-authentication-agent-1) &

# Wallpaper
nitrogen --restore &

